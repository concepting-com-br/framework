<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE artifacts>

<artifacts xmlns:concepting="default namespace"
           xmlns:jsp="default namespace"
           xmlns:c="default namespace">
	<artifact name="br.com.innovativethinking.framework.model.util.ModelUtil.getUrlByModel(#{clazz})">
		&lt;%@ page errorPage="/WEB-INF/jsp/error.jsp"%&gt;
		&lt;%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%&gt;
		&lt;%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%&gt;
		&lt;%@ taglib uri="http://java.sun.com/jsp/jstl/functions" prefix="fn"%&gt;
		&lt;%@ taglib uri="https://www.innovativethinking.com.br/framework/ui/components" prefix="concepting"%&gt;
    	<set var="actionFormName" value="br.com.innovativethinking.framework.controller.form.util.ActionFormUtil.getActionFormIdByModel(#{clazz})"/>
   		<set var="resourcesId" value="br.com.innovativethinking.framework.model.util.ModelUtil.getResourcesIdByModel(#{clazz})"/>
   		
		<concepting:page title="${loginSession.systemModule.title} ${loginSession.systemModule.version}">
			<jsp:include page="/WEB-INF/jsp/header.jsp"/>
			<concepting:form name="@{actionFormName}" resourcesId="@{resourcesId}">
				<table class="content">
					<tr>
						<td></td>
					</tr>
				</table>
				<jsp:include page="/WEB-INF/jsp/footer.jsp"/>
			</concepting:form>
		</concepting:page>
	</artifact>
</artifacts>